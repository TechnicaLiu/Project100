page {
  background: #f3f3f3;
}
.listBox{
  width: 100%;
  padding: 0rpx 30rpx;
}
.item{
  display: flex;
  background-color: #fff;
  border-radius: 5px;
  margin-top: 10px;
  overflow: hidden;
  padding: 5px 5px;
}
.img{
  width: 70px;
  height: 60px;
  border-radius: 5px;
  overflow: hidden;
  margin-right: 10px;
}
.img image{
  width: 100%;
  height: 100%;
}
.info{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.info h3{
  font-size: 14px;
  margin-bottom: 5px;
}
.info p {
  font-size: 11px;
  color: #424242;
}
.boxClass{
  height: 120px;
  display: flex;
  flex-direction: column;
  align-content: center;
}
.imageClass {
  width: 30px;
  height: 30px;
}
.titleClass{
  font-size: 15px;
}
.boxClass{
  border-radius: 8px;
}
.numClass{
  float: right;
}
.time{
  float: right;
}

.clear{
  position: fixed;
  bottom: 30px;
  width: 100%;
  text-align: center;
}